# ============================================
# CMPC LIMS Frontend Environment Variables
# ============================================
# Copy this file to .env and fill in the values

# API Configuration
# Point this to your backend Railway URL
VITE_API_URL=https://your-backend-url.railway.app/api

# Application
VITE_APP_NAME=CMPC LIMS
VITE_APP_VERSION=1.0.0

# Feature Flags (optional)
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_DEBUG=false

# ============================================
# Railway-specific notes:
# ============================================
# - Set VITE_API_URL to your Railway backend URL after deployment
# - Make sure to include /api at the end of the backend URL
# - All VITE_ variables are embedded at build time
